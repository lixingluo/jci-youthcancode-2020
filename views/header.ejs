        <button class="ycc-block ycc-teal ycc-padding-8 ycc-section ycc-right" onclick="location.href='/'">SUBMIT <i class="fa fa-check-circle"></i></button>
        <button class="ycc-block ycc-teal ycc-padding-8 ycc-section" onclick="document.getElementById('ticketModal').style.display='none'">CANCEL <i class="fa fa-times-circle"></i></button>
